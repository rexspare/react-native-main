{
  "name": "@tigra/client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-debug-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res  && cd android && ENVFILE=.env.test ./gradlew assembleDebug && cd .. ",
    "android": "react-native run-android",
    "awt": "SET ENVFILE=.env.test && react-native run-android",
    "aws": "SET ENVFILE=.env.staging && react-native run-android",
    "ios": "react-native run-ios",
    "start": "NODE_OPTIONS=--openssl-legacy-provider react-native start",
    "test": "jest",
    "postinstall": "react-native link",
    "dist:ios-staging": "cd ios && fastlane beta --env staging && cd ../",
    "dist:ios-prod": "cd ios && fastlane release --env production && cd ../",
    "dist:android-staging": "cd android && fastlane bump_version_code --env staging && fastlane beta --env staging && cd ../",
    "dist:android-prod": "cd android && fastlane bump_version_code --env production && fastlane release --env production && cd ../"
  },
